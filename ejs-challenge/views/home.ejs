<%- include('partials/header.ejs') %>
<h1>HOME</h1>
<p><%- homeStartingContent %> </p>
<% for(var i = 0 ; i < posts.length;i++){ %>
    <% var url="posts/"+posts[i].post; %>
    <h1><%- posts[i].post%></h1>   
    <% const truncatedContent = posts[i].body.substring(0, 100)+"...";%> 
    <p><%- truncatedContent %> <a  href="<%= url %>">Read more</a> </p>
<% } %>
<%- include('partials/footer.ejs') %>
